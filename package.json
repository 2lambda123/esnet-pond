{
  "name": "pondjs",
  "version": "0.5.0",
  "main": "lib/entry",
  "description": "A timeseries library build on top of immutable.js",
  "scripts": {
    "lint": "eslint src test/tests website/modules/*.jsx",
    "test": "npm run lint && npm run build && mocha --compilers js:babel-core/register",
    "test-only": "npm run build && mocha --compilers js:babel-core/register",
    "build-docs": "jsdoc -X ./src/ > website/docs.json",
    "build": ". ./scripts/build.sh",
    "build-website": "webpack --config webpack.website.config.js",
    "deploy-website": "gh-pages-deploy",
    "start-website": "webpack-dev-server --content-base website/ --config webpack.website.config.js",
    "start-tester": "webpack-dev-server --config webpack.test.config.js --content-base ./devserver --port 9500 --colors"
  },
  "pre-commit": [
    "lint",
    "test-only",
    "build"
  ],
  "gh-pages-deploy": {
    "staticpath": "website",
    "prep": [
      "build-website"
    ]
  },
  "license": "BSD-3-Clause-LBNL",
  "dependencies": {
    "avsc": "^3.4.1",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-runtime": "^6.5.0",
    "immutable": "^3.6.4",
    "immutable-devtools": "0.0.4",
    "install": "^0.6.1",
    "moment": "^2.9.0",
    "npm": "^3.8.6",
    "underscore": "^1.8.2"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.0",
    "eslint-config-esnet": "^0.1.0",
    "eslint-plugin-babel": "^2.1.1",
    "eslint-plugin-react": "^3.9.0",
    "file-loader": "^0.8.5",
    "gh-pages-deploy": "^0.3.0",
    "jsdoc": "^3.4.0",
    "json-loader": "^0.5.4",
    "mocha": "^2.2.5",
    "mocha-loader": "^0.7.1",
    "pre-commit": "^1.1.2",
    "raw-loader": "^0.5.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-markdown": "^1.0.5",
    "react-router": "^2.2.4",
    "react-timeseries-charts": "git+ssh://git@github.com/esnet/react-timeseries-charts.git#v0.9",
    "ringjs": "0.0.1",
    "style-loader": "^0.13.0",
    "scroll-behavior": "^0.3.0",
    "url-loader": "^0.5.7",
    "wallaby-webpack": "0.0.11",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "keywords": [
    "timeseries",
    "immutable"
  ]
}
