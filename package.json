{
  "name": "@esnet/pond",
  "version": "0.1.3",
  "description": "A timeseries library build on top of immutable.js",
  "options": {
    "mocha": "--require scripts/mochaboot test/*.js"
  },
  "scripts": {
    "prepublish": "npm test && npm run build",
    "lint": "eslint src",
    "test": "npm run lint && mocha $npm_package_options_mocha",
    "test-only": "mocha $npm_package_options_mocha",
    "build": "rm -rf lib/* && babel src --optional runtime --stage 0 --out-dir lib",
    "build-website": "webpack --config webpack.website.config.js",
    "start-website": "webpack-dev-server  --config webpack.website.config.js",
    "start-tester": "webpack-dev-server --config webpack.test.config.js --content-base ./devserver --port 9500 --colors"
  },
  "babel": {
    "optional": [
      "runtime",
      "es7.objectRestSpread"
    ]
  },
  "license": "Modified BSD License",
  "dependencies": {
    "immutable": "^3.6.4",
    "moment": "^2.9.0",
    "underscore": "^1.8.2",
    "babel-runtime": "~5.8.3"
  },
  "devDependencies": {
    "react": "^0.13.x",
    "react-router": "^0.13.x",
    "react-markdown-el": "^1.0.2",
    "bundle-loader": "^0.5.0",
    "url-loader": "^0.5.5",
    "file-loader": "^0.7.2",
    "jsx-loader": "^0.13.x",
    "json-loader": "^0.5.1",
    "style-loader": "^0.8.0",
    "raw-loader": "^0.5.1",
    "css-loader": "^0.9.0",
    "babel": "^5.8.3",
    "babel-loader": "^5.0.0",
    "babel-core": "^5.1.13",
    "babel-eslint": "4.0.5",
    "chai": "^3.0.0",
    "mocha": "^2.2.5",
    "mocha-loader": "^0.7.1",
    "webpack": "^1.7.3",
    "webpack-dev-server": "^1.6.5",
    "eslint": "^0.24.0"
  }
}
